.profileContainer
{
	display:flex;
	flex-direction:column;
}
.nameImgHolder{
	display: flex;
	align-items:center;
	width:fit-content;
	padding:0.9rem;
	font-size:0.7rem;
	transform:translateX(15vw);
	margin-top:3rem;
	margin-bottom:3rem;
}
.UserProfilespan{
	color : gray;
	text-align: center;
}
.nameImgHolder img{
	border-radius : 50%;
	width:5rem;
	height:5rem;
	
}
.imageContainer{
	margin-right:1rem;
}
.nameContainer{
	display:flex;
	flex-direction:column;
	align-items:center;
}
.infoContainerTop{
	display:flex;
	align-items:center;
	justify-content : space around;
}
.infoContainerContent{
	display:flex;
	align-items : center;
	justify-content:center;
	flex-direction:column;
	margin-left : 15vw;;
}
.UserProfileh2{
	text-align:center;
	font-weight : lighter;
}

.TypeChartBar
{
	transform:translateX(15vw);
	margin-bottom:3rem;
	
}
.pbsTime
{
	display:flex;
	justify-content:space-between;
	font-size:0.7rem;
	width:fit-content;
	transform:translateX(15vw);
	margin-top: 3rem;
	margin-bottom:3rem;
}
.groupContainer
{
	display:flex;
}
.group{
	width:8.75vw;
	display:flex;
	flex-direction : column;
	justify-content : center;
	align-items:center;
	
}

.moredetailsgroup
{
	width:17.5vw;
	display:flex;
	flex-direction : column;
	justify-content : center;
	align-items:center;
}
.moreDetails 
{
	display:flex;
	justify-content:space-between;
	transform:translateX(15vw);
	margin-bottom:3rem;
	margin-top:3rem;
	width : fit-content;
}
.profileTable
{
	transform:translateX(15vw);
	width:fit-content;
	
}
.table-container-inside table
{
	width:52.2vw;
	margin-right:1rem;
}

@media all and (max-width :80rem){
	.nameImgHolder
	{
		transform:translateX(14vw);
	}
   .infoContainerTop
   {
	 flex-direction:column;
   }
	.pbsTime
	{
		flex-direction : column;
		transform:translateX(13.5vw);
	}
	.group
	{
		width:17.5vw;
	}
	.infoContainerTop
	{
		width:35.3vw;
	}
	.moreDetails{
		transform:translateX(13.5vw);
	}
	.profileTable
	{
		transform:translateX(12.5vw);
	}

}
@media all and (max-width :60rem){
.pbsTime{
	transform:translateX(7.5vw);
}
.nameImgHolder img
{
	width:15vw;
	height:15vw;
}
	.nameImgHolder
	{
		flex-direction:column;
	}
   .infoContainerContent{
	margin-left :0;
}
	.nameImgHolder
	{
		transform:translateX(15vw);
	}
	.nameContainer
	{
		margin-bottom:1rem;
	}
	.UserProfileh2
	{
		margin-top : 0.7rem;
		margin-bottom : 0.7rem;
	}
	.moreDetails{
		transform:translateX(6.5vw);
	}
	.profileTable
	{
		transform:translateX(6.5vw);
	}

}
@media all and (max-width :40rem)
{
	
	.profileTable
	{
		transform:translateX(2.5vw);
	}
}
@media all and (max-width :30rem)
{
	
	.nameImgHolder
	{
		transform:translateX(-20px;);
	}
	.profileTable
	{
		transform:translateX(-2.5vw);
	}
}


